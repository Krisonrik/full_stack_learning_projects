<%- include('header') %>
<body>
  <div class="box" id="heading">
    <h1><%= kindOfDay %>!</h1>
    <% if (listName != "default"){ %> <!-- -->
    <h2><%= listName %> list</h2>
    <%}%> 
  </div>
  <div class="box">
    <% for(let i=0; i < newListItems.length; i++){%>
    <div class="item">
      <input type="checkbox" />
      <p><%= newListItems[i] %></p>
    </div>
    <% }%>
    <form class="item" action="/" method="post">
      <input
        type="text"
        name="newItem"
        placeholder="New Item"
        autocomplete="off"
      />
      <button type="submit" name="listButtonName" value=<%= listName %> class="">+</button>
    </form>
  </div>
</body>
<%- include('footer') %>
